html,
body
	font-family PTSans-Regular
	color #444444

a
	text-decoration none

b
	font-weight bold
strong
	font-weight 700
	
.cont-fs
	font-size pixelsToEm(24px)
	line-height 30px
	
.page-title-wrapper
	background-size cover
	padding 97px 50px
	.page-title
		color #ffffff
		font-size pixelsToEm(35)
		font-weight 700
		text-shadow 0 2px 2px rgba(0,0,0,0.3)

.title-st-1
	font-size pixelsToEm(30)
	line-height 30px
	font-weight 700
	color #366d8f

.list-st1
	li
		padding-left 20px
	li:before
		display inline-block
		content ''
		width 7px
		height 7px
		line-height 30px
		vertical-align middle
		border-radius 50%
		background-color #366d8f
		margin-left -18px
		margin-right 11px
	
	