.prise-extend

	.prise-extend__table.mb-20.clear
		.prise-extend__table-item.side-bar

			.prise-extend__table-heading.uppercase.hide-on-small-and-down
				span.first-line It's time to #[br]
				span.second-line Choose #[br]
				span.third-line Your Course

			ul.prise-extend__list._content
				-list = [{text: '24/7 Tutor Support', icon: 'fa-life-ring'}, {text: 'Tech Support', icon: 'fa-cogs'}, {text: 'Full Library Access', icon: 'fa fa-book'}, {text: 'Skills Based Units', icon: 'fa-user-plus'}, {text: 'Intensive Grammar Units', icon: 'fa-font'}, {text: 'Comprehensive Phonology', icon: 'fa-bold'}, {text: 'Curriculum Development', icon: 'fa-level-up'}, {text: 'Designing Testing Systems', icon: 'fa-pencil-square-o'}, {text: 'Popularity', icon: 'fa-star'}]
				each elem in list
					li.prise-extend__list-item
						//- img(src="assets/images/prise-extend/"+ elem.img alt="img")
						i.fa(class=elem.icon)
						span
							=elem.text

			.prise-extend__footer.hide-on-small-and-down
				ul.prise-extend__list._footer
					li.prise-extend__list-item
						a(href=jv0)
							i(class="fa fa-hand-o-right" aria-hidden="true")
							span Talk to a TEFL Expert
					li.prise-extend__list-item
						a(href=jv0)
							i(class="fa fa-hand-o-right" aria-hidden="true")
							span Our special offers
					
					li.prise-extend__list-item
						a(href=jv0)
							i(class="fa fa-hand-o-right" aria-hidden="true")
							span FAQs
		
