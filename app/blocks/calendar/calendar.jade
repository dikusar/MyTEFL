- calendar = [{date: '28', month:'September', year: '2016'}, {date: '26', month:'October', year: '2016'}, {date: '30', month:'December', year: '2016'}, {date: '28', month:'January', year: '2017'}, {date: '1', month:'February', year: '2017'}, {date: '29', month:'March', year: '2017'}, {date: '4', month:'April', year: '2017'}, {date: '2', month:'MAy', year: '2017'}, {date: '1', month:'June', year: '2017'}];
.calendar
	each item in calendar
		.calendar__row.uppercase
			.calendar__item
				span.calendar__date
					=item.date
				span.calendar__month
					=item.month
			.calendar__year
					=item.year
				
